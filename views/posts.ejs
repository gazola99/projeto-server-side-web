<!DOCTYPE html>
<html>

<head>
    <title><%= title %></title>
    <%- include('imports'); -%>
</head>

<body>
    <%- include('navbar'); -%>

    <div class="container">
        <h1><%= title %></h1>
        <div class="row">
            <% posts.forEach((post) => { %>
            <div class="col-4">
                <%- include('post_card', { post: post }); -%>
            </div>
            <% }); %>
        </div>
    </div>

</body>

</html>